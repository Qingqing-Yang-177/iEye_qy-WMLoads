
load('/deathstar/home/wayne/Dropbox/PatientsMGS/Control_1/ii_stats.mat')

for r = 1:9   
    load(['/deathstar/home/wayne/Dropbox/PatientsMGS/Control_1/DATA_Processed/mgs_' num2str(r) '.mat'])
    
    ii_selectbyvalue('XDAT',1,3);
    dels = ii_cfg.cursel(:,2) - ii_cfg.cursel(:,1)
    
    b1 = find(delays<3400);
    b2 = find(delays>3400 & delays<3900);
    b3 = find(delays>3900 & delays<4400);
    b4 = find(delays>4400 & delays<4900);
    b5 = find(delays>4900);
    
    dp(b1) = 3;
    dp(b2) = 3.5;
    dp(b3) = 4;
    dp(b4) = 4.5;
    dp(b5) = 5;
    
    dp = dp';
    
    ii_stats(r).dp = dp;
end